{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "build/**"
            ]
        },
        "lint": {},
        "start": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "prerender": {
            "cache": false,
            "persistent": false
        }
    }
}